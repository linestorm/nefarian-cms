{% extends 'LineStormFileBundle::layout.html.twig' %}

{% block head %}
    {% block requirejs %}
        {{
            include('LineStormCmsBundle::requirejs.html.twig', {
                requirejs_module: ['cms_main', 'cms_file', 'cms_file_treebrowser']
            })
        }}
    {% endblock %}

    {% stylesheets
        filter="compass" output="cms/file/css/file.css"
        "@LineStormFileBundle/Resources/assets/sass/file.scss"
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block module %}
    {% include 'LineStormFileBundle:Form:form-multiple.html.twig' %}
{% endblock %}
