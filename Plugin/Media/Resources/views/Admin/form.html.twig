{% extends 'LineStormMediaBundle::layout.html.twig' %}

{% block head %}
    {% block requirejs %}
        {{
            include('LineStormCmsBundle::requirejs.html.twig', {
                requirejs_module: ['cms_main', 'cms_media', 'cms_media_treebrowser']
            })
        }}
    {% endblock %}

    {% stylesheets
        filter="compass" output="cms/media/css/media.css"
        "@LineStormMediaBundle/Resources/assets/sass/media.scss"
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block module %}
    {% include 'LineStormMediaBundle:Form:form-multiple.html.twig' %}
{% endblock %}
